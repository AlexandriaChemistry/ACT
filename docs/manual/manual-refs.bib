@article{Amdur1958a,
    author = {Amdur, I. and Mason, E. A.},
    title = {Properties of Gases at Very High Temperatures},
    journal = {Phys. Fluids},
    volume = {1},
    OPTnumber = {5},
    pages = {370-383},
    year = {1958},
    month = {09},
    abstract = {Known intermolecular force laws are used to calculate virial coefficients of the equation of state and the transport properties at very high temperatures for the rare gases and for molecular nitrogen. The crucial force laws which enable the calculations to be carried to high temperatures are those derived from molecular beam scattering measurements, which are valid at close distances of molecular approach. The calculations cover the temperature range from 1000o to 15 000oK, but take into account only the translational degrees of freedom and neglect the effects of excitation, dissociation, and ionization. The treatment of mixtures is outlined, and illustrated by calculations for the binary system helium-argon. The methods which are used offer an approach to the problem of obtaining reasonably accurate estimates of gas properties at temperatures so high that direct experiments would be extremely difficult, if not impossible.},
    issn = {0031-9171},
    doi = {10.1063/1.1724353},
    OPTurl = {https://doi.org/10.1063/1.1724353},
    OPTeprint = {https://pubs.aip.org/aip/pfl/article-pdf/1/5/370/12270532/370\_1\_online.pdf},
}

