@ARTICLE{Verstraelen2009a,
  author =       {Verstraelen, Toon and {Van Speybroeck}, Veronique
                  and Waroquier, Michel},
  title =        {{The electronegativity equalization method and the
                  split charge equilibration applied to organic
                  systems: Parametrization, validation, and
